    <Member MemberName="DateTime">
      <MemberSignature Language="C#" Value="DateTime" />
      <MemberSignature Language="ILAsm" Value=".field public static literal valuetype System.Management.CimType DateTime = int32(101)" />
      <MemberSignature Language="DocId" Value="F:System.Management.CimType.DateTime" />
