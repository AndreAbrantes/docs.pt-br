      <MemberSignature Language="C#" Value="public string Indices { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance string Indices" />
      <MemberSignature Language="DocId" Value="P:System.Windows.Documents.Glyphs.Indices" />
      <MemberType>Property</MemberType>
